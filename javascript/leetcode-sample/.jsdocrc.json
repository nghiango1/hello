{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": [ "index.mjs", "src/" ],
        "exclude": [ "node_modules/", "dist/"],
        "includePattern": ".+\\.(js|mjs)(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}

