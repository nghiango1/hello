{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:jsdoc/recommended-error"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "jsdoc/require-description": "warn",
        "valid-jsdoc": "error"
    },
    "plugins": [
        "jsdoc"
    ]
}
